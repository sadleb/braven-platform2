<% content_for :additional_container_css, 'embedded_linked_in_authorization_container' %>

<!-- 
  This page renders a static LinkedIn sign-in button.
  It can be used in an i-frame in Content Editor where we need to get the 
  user's permission to access their linked in data.
-->
<%= react_component('LinkedIn/LinkedInAuthorizationButton', {
  url: linked_in_auth_path(lti_launch_id: @lti_launch.id),
})%>
